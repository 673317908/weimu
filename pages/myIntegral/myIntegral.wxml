<import src="../../templates/login-popup.wxml" />

<view class="mypoints">
  <view class="mypoints-num">{{memberUserInfo.integral || 0}}</view>
  <view class="mypoints-title">
    <text>我的积分</text>
    <text class="icon-question" bindtap="toIntegralDes">?</text>
  </view>
</view>

<view class="listBox">
    <block wx:key="id" wx:for="{{integral}}">
  <view class="listItem">
      <view class="left">
        <text> {{item.itemname}}</text>
        <view class="time">{{item.integralDate}} </view>
      </view>
      <view class="right">
        <text class="add-points" wx:if="{{item.flag=='add'}}">+{{item.integral}}分</text>
         <text class="deduct-points" wx:else>-{{item.integral}}分</text>
      </view>
  </view>
      </block>
</view>

