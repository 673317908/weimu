<!--基础元素-->
<template name="wxParseVideo">
  <!--增加video标签支持，并循环添加-->
  <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
    <video class="{{item.classStr}} wxParse-{{item.tag}}-video" src="{{item.attr.src}}" poster="{{item.attr.poster}}"></video>
  </view>
</template>
<template name="wxParseAudio">
  <!--增加audio标签支持，并循环添加-->
  <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
    <audio poster="{{item.attr.poster}}" name="{{item.attr.name}}" author="{{item.attr.author}}" src="{{item.attr.src}}" id="myAudio" controls loop></audio>
  </view>
</template>
<template name="wxParseImg">
  <image class="{{item.classStr}} wxParse-{{item.tag}}" show-menu-by-longpress="true" lazy-load="true" data-from="{{item.from}}" data-src="{{item.attr.src}}" data-idx="{{item.imgIndex}}" src="{{item.attr.src}}" bindload="wxParseImgLoad" bindtap="wxParseImgTap"
    mode="widthFix" style="width:100%;" />
</template>
<!--33行加入text组件，用于复制文字-->
<template name="WxEmojiView">
  <view class="WxEmojiView wxParse-inline" style="{{item.styleStr}}">
    <block wx:for="{{item.textArray}}" wx:key="index">
      <block class="{{item.text == '\\n' ? 'wxParse-hide':''}}" wx:if="{{item.node == 'text'}}">
        <text>{{item.text}}</text>
      </block>
      <block wx:elif="{{item.node == 'element'}}">
        <image class="wxEmoji" src="{{item.baseSrc}}{{item.text}}" />
      </block>
    </block>
  </view>
</template>

<!--跳转短代码-->
<template name="wxParseMinapperShortcode">
  <view class="wmcode-view" data-appid="{{item.attr.appid}}" data-redirectype="{{item.attr.redirectype}}" data-url="{{item.attr.url}}" data-path="{{item.attr.path}}" bindtap="wxParseToRedict">
    <view class="wmcode-inner">
      <view class="wmcode-img">
        <image mode="aspectFill" src="{{item.attr.poster}}" />
      </view>

      <view class="wmcode-content">
        <view class="wmcode-text-box">
          <view class="wmcode-text-title">{{item.attr.title}}</view>
          <view class="wmcode-text-des" wx:if="{{item.attr.description !=''}}">{{item.attr.description}}</view>
        </view>

        <view class="wmcode-btn-box">
          <view class="wmcode-price" wx:if="{{item.attr.codetype=='goods'}}">{{item.attr.price ? '￥' + item.attr.price : ''}}</view>
          <view class="wmcode-price" wx:else />

          <view class="wmcode-btn-goods" wx:if="{{item.attr.codetype=='goods'}}">{{item.attr.buttontext}}</view>
          <view class="wmcode-btn" wx:if="{{item.attr.codetype !='goods'}}">{{item.attr.buttontext}}</view>
        </view>
      </view>

    </view>
  </view>
</template>

<!--广告短代码 -->
<template name="wxParseMinapperad">
  <view class="minapper-ad">
    <ad unit-id="{{item.attr.unitid}}" ad-type="{{item.attr.adtype}}" ad-theme="{{item.attr.adtheme}}" ad-intervals="{{item.attr.adintervals}}" binderror="adbinderror" bindload="adbindload" bindload="adbindload" bindclose="bindclose"> </ad>
  </view>
</template>

<!--地图短代码 -->
<template name="wxParseMinappermap">
  <view class="minapper-ad" bindtap="openmap" data-lat="{{item.attr.latitude}}" data-address="{{item.attr.title}}" data-lng="{{item.attr.longitude}}">
    <map id="myMap" style="width: 100%; height: 420rpx;" latitude="{{item.attr.latitude}}" longitude="{{item.attr.longitude}}" markers="{{item.attr.markers}}" show-location></map>
  </view>
   <view wx:if="{{item.attr.title !=''}}" bindtap="openmap" data-lat="{{item.attr.latitude}}" data-address="{{item.attr.title}}" data-lng="{{item.attr.longitude}}" class="address">位置：{{item.attr.title}}</view>
</template>

<!--腾讯视频短代码 -->
<template name="wxParseMinapperQQvideo">
  <view class="wxParse-video" wx:if="{{item.attr.vid !=''}}">
    <txv-video vid="{{item.attr.vid}}" playerid="id{{item.attr.vid}}" width="100%" height="100%" autoplay="{{false}}">
    </txv-video>
  </view>
</template>


<!--相册滑动短代码 -->
<template name="wxParseMinapperGallery">
  <swiper indicator-dots indicator-color="rgba(255, 255, 255, .3)" indicator-active-color="rgba(255, 255, 255, .8)" interval="10000" autoplay="true" circular="true" style="height: 600rpx">
    <block wx:key="id" wx:for="{{item.attr.images}}" wx:index="{{index}}">
      <swiper-item id="{{item.id}}" index="{{index}}" data-imgallsrc="{{item.allimages}}" data-imgsrc="{{item.imageurl}}" data-imgsrc="{{item.imageurl}}" bindtap="previewImage">
        <image mode="aspectFill" style="height: 600rpx" src="{{item.imageurl}}" />
      </swiper-item>
    </block>
  </swiper>
</template>


<template name="WxParseBr">
  <text>\n</text>
</template>

<template name="wxParseStyle">
  <text></text>
</template>
<!--入口模版-->

<template name="wxParseNocopy">
  <block wx:for="{{wxParseData}}" wx:key="index">
    <template is="wxParse0" data="{{item}}" />
  </block>
</template>


